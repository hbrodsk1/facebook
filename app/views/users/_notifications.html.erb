<ul>
    <% FriendRequest.where(friend_id: current_user.id).find_each do |request| %>
    <h4>You have new friend requests from:</h4>
    <li>
      <%= request.user.email %>
      <%= link_to "Accept",  friend_request_path(friend_id: request.user_id), method: "put" %>
      <%= link_to "Decline", friend_request_path(friend_id: request.user_id), method: "delete" %>
    </li>
  <% end %>
</ul>
